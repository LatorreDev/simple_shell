#include "microshell.h"

int built_ins(char **args, char *buffer)
{
	int cont = 0;
	bin bin_list[] = {
		{"exit", exit_function},
		{"env", printenv_function},
		{NULL, NULL}
	};
	if (args[0] == NULL)
	{
		return (-1);
	}
	while (bin_list[idx].name != NULL)
	{
		if (_strcmp(args[0], bin_list[idx].name) == 0)
			bin_list[idx].func(args, buffer);
		idx++;
	}
	if (bin_list[idx].name == NULL)
		return (-1);
	return (0);
}

